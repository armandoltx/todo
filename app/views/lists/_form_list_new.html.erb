<%= form_for List.new do |f| %>
    <div class="form-group">
      <%= f.text_field :title, placeholder: "title", :autofocus => true, :required => true, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.text_field :category, placeholder: "category", :required => true, :class => 'form-control' %>
    </div>
    <%= f.submit %>
<% end %>
