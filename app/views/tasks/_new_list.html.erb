<h1>Lists#new</h1>
<%= form_for List.new do |f| %>
    <div class="form-group">
      <%= f.label :title %>
      <%= f.text_field :title, :autofocus => true, :required => true, :class => 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.label :category %>
      <%= f.text_field :category, :required => true, :class => 'form-control' %>
    </div>
    <%= f.submit %>
<% end %>
